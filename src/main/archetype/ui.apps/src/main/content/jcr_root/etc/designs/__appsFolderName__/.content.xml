<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        jcr:primaryType="nt:unstructured"
        jcr:title="${siteName} Design"
        sling:resourceType="wcm/core/components/designer">
        <page jcr:primaryType="nt:unstructured">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys"
                components="[/libs/foundation/components/image,/libs/wcm/foundation/components/textimage,/libs/wcm/foundation/components/parsys/colctrl,/libs/wcm/foundation/components/text,/libs/wcm/foundation/components/content/title,/apps/${appsFolderName}/components/content/servicecomponent]">
                <colctrl
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/parsys/colctrl"
                    layouts="2;cq-colctrl-lt0&#x9;2 Columns (50%, 50%)&#xa;"/>
            </par>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/logo"
                absParent="1">
                <image/>
            </logo>
        </page>
    </jcr:content>
</jcr:root>

{
  "cssRules": {
    ".afHeader": {
      "color": "rgba(150,150,150,1)",
      "top": "0px",
      "right": "0px",
      "left": "0px",
      "border-bottom-width": "1px",
      "height": "90px",
      "vertical-align": "text-top",
      "background-color": "rgba(245,245,245,1)",
      "border-color": "rgba(230,230,230,1)",
      "margin-bottom": "16px",
      "border-style": "solid"
    },
    ".logoImage img": {
      "height": "60px",
      "margin": "15px 12px 0px 0px",
      "max-width": "100%"
    },
    ".guideTextDraw.headerText": {
      "font-size": "18px",
      "color": "rgba(85,85,85,1)",
      "vertical-align": "top",
      "height": "70px"
    },
    "body": {
      "font-family": "arial",
      "margin": "0px"
    },
    ".guideformtitle": {
      "font-size": "24px",
      "color": "rgba(102,102,102,1)",
      "padding": "0px 12px"
    },
    ".guideFieldNode:not(.guideButton):not(.guideTextDraw):not(.guideChart)": {
      "margin": "4px 0px",
      "padding": "8px 12px 0px 8px",
      "border-width": "0px 0px 0px 4px",
      "border-style": "solid",
      "border-color": "rgba(255,255,255,0)"
    },
    "label": {
      "color": "rgba(102,102,102,1)",
      "font-size": "16px",
      "margin": "0px 0px 2px 0px"
    },
    "input": {
      "border-style": "solid",
      "border-width": "1px",
      "border-radius": "4px",
      "border-color": "rgba(204,204,204,1)",
      "height": "40px",
      "color": "rgba(85,85,85,1)",
      "font-size": "14px",
      "padding": "10px",
      "background-color": "rgba(255,255,255,1)"
    },
    "select": {
      "border-style": "solid",
      "border-width": "1px",
      "border-radius": "4px",
      "border-color": "rgba(204,204,204,1)",
      "height": "40px",
      "color": "rgba(85,85,85,1)",
      "font-size": "14px",
      "padding": "10px",
      "background-color": "rgba(255,255,255,0.5)"
    },
    "textarea": {
      "border-style": "solid",
      "border-width": "1px",
      "border-radius": "4px",
      "border-color": "rgba(204,204,204,1)",
      "color": "rgba(85,85,85,1)",
      "font-size": "14px",
      "padding": "10px",
      "background-color": "rgba(255,255,255,1)"
    },
    ".multiline.textField textArea": {
      "height": "80px"
    },
    ".guideFieldWidget.guideNumericStepper": {
      "min-width": "200px",
      "border-width": "1px",
      "border-style": "solid",
      "border-radius": "4px",
      "border-color": "rgba(204,204,204,1)",
      "background-color": "rgba(255,255,255,1)"
    },
    ".guideFieldWidget.guideNumericStepper input[type='text']": {
      "border-width": "0px",
      "text-align": "center"
    },
    ".guideFieldWidget.guideNumericStepper a.ui-spinner-button.ui-spinner-up": {
      "width": "40px",
      "height": "40px",
      "border-radius": "0px 4px 4px 0px"
    },
    ".guideFieldWidget.guideNumericStepper a.ui-spinner-button.ui-spinner-down": {
      "width": "40px",
      "height": "40px",
      "border-radius": "4px 0px 0px 4px"
    },
    ".dropDownList  select": {
      "height": "40px",
      "border-width": "1px",
      "border-style": "solid",
      "border-color": "rgba(204,204,204,1)",
      "padding": "10px",
      "border-radius": "4px"
    },
    ".guideFieldWidget.guideDateInputWidget .comb-datefield label": {
      "font-size": "12px",
      "text-transform": "uppercase",
      "padding": "0px",
      "margin": "8px 0px 2px 0px",
      "color": "rgba(136,136,136,1)"
    },
    ".guideFieldWidget.guideDateInputWidget input[type='text'].comb-form-group-day": {
      "width": "58px",
      "text-align": "center"
    },
    ".guideFieldWidget.guideDateInputWidget input[type='text'].comb-form-group-month": {
      "width": "58px",
      "text-align": "center",
      "text-transform": "uppercase"
    },
    ".guideFieldWidget.guideDateInputWidget input[type='text'].comb-form-group-year": {
      "width": "64px",
      "text-align": "center"
    },
    ".guideFieldWidget.afTermsAndConditions": {
      "border-width": "0px"
    },
    ".afTncContentArea": {
      "background-color": "rgba(255,255,255,1)",
      "overflow": "auto",
      "border-style": "solid",
      "border-width": "1px",
      "border-color": "rgba(204,204,204,1)",
      "border-radius": "4px",
      "font-size": "14px",
      "color": "rgba(150,150,150,1)",
      "max-height": "160px"
    },
    ".guide-tnc-checkbox": {
      "margin-top": "0px"
    },
    ".guideFieldDescription.short": {
      "font-size": "14px",
      "color": "rgba(150,150,150,1)"
    },
    ".guideFieldDescription.long": {
      "margin": "4px 0px",
      "border-radius": "4px",
      "font-size": "14px",
      "color": "rgba(102,102,102,1)",
      "background-color": "rgba(0,0,0,0.05)",
      "padding": "2px 16px"
    },
    ".guideFieldError": {
      "color": "rgba(235,60,54,1)",
      "margin": "0px",
      "padding": "4px 0px",
      "font-size": "14px"
    },
    "button:not(.af-icon-button)": {
      "border-style": "solid",
      "border-width": "2px",
      "padding": "10px 16px",
      "font-size": "14px",
      "border-radius": "24px",
      "background-color": "rgba(255,255,255,0)",
      "color": "rgba(102,102,102,1)",
      "border-color": "rgba(102,102,102,1)",
      "margin": "0px 0px 0px 12px"
    },
    "button.submit:not(.af-icon-button)": {
      "background-color": "rgba(102,102,102,1)",
      "border-color": "rgba(102,102,102,1)",
      "color": "rgba(255,255,255,1)"
    },
    ".guideGridFluidLayout": {
      "color": "rgba(102,102,102,1)"
    },
    ".guideGridFluidDescription.guidePanelDescription": {
      "margin": "0px 12px 24px 12px",
      "font-size": "20px",
      "color": "rgba(102,102,102,1)"
    },
    ".guideTabbedPanelLayout": {
      "border-width": "0px",
      "border-style": "solid",
      "border-color": "rgba(8,177,207,1)"
    },
    ".guideTabbedPanelLayout .tab-navigators.tab-navigators-vertical": {
      "margin": "0px",
      "padding": "0px",
      "background-color": "rgba(241,241,241,1)",
      "border-color": "rgba(230,230,230,1)",
      "border-style": "solid"
    },
    ".guideTabbedPanelLayout .tab-navigators.tab-navigators-vertical .tab-navigators-vertical": {
      "border-width": "0px"
    },
    ".afTabLeftPanel": {
      "padding": "16px 56px",
      "border-style": "solid",
      "background-color": "rgba(250,250,250,1)",
      "border-color": "rgba(230,230,230,1)"
    },
    ".guidePanelDescription.long": {
      "padding": "2px 16px",
      "margin": "4px 0px 0px 0px",
      "background-color": "rgba(0,0,0,0.05)",
      "font-size": "14px",
      "border-radius": "4px"
    },
    ".guideTableNode table": {
      "margin": "4px 12px 12px 12px",
      "border-width": "0px",
      "border-color": "rgba(255,255,255,1)"
    },
    ".guideTableRuntimeAddControl": {
      "height": "24px",
      "width": "24px",
      "margin": "16px 0px 16px 16px"
    },
    ".guideTableRuntimeDeleteControl": {
      "height": "24px",
      "width": "24px",
      "margin": "16px"
    },
    ".guideTableNode div.guidePanelDescription": {
      "margin": "0px 12px",
      "color": "rgba(102,102,102,1)"
    },
    ".guideTableNode table .guideTableRowNode.guideTableHeader": {
      "background-color": "rgba(165,165,165,1)",
      "border-bottom-style": "solid",
      "border-bottom-width": "3px",
      "height": "70px"
    },
    ".guideTableNode table .guideTableRowNode.guideTableHeader th": {
      "font-weight": 700,
      "height": "57.2px",
      "color": "rgba(255,255,255,1)",
      "text-align": "left",
      "background-color": "rgba(165,165,165,1)",
      "border-top-style": "solid",
      "border-right-style": "solid",
      "border-width": "1px 1px 0px 0px",
      "border-color": "rgba(255,255,255,1)"
    },
    ".guideTableNode table tbody tr:nth-of-type(even)": {
      "background-color": "rgba(240,240,240,1)"
    },
    ".guideTableNode table tbody:nth-of-type(odd) tr:nth-of-type(odd)": {
      "background-color": "rgba(225,225,225,1)"
    },
    ".guideTableNode table td": {
      "text-align": "left",
      "border-style": "solid solid none none",
      "border-width": "1px 1px 0px 0px",
      "border-color": "rgba(255,255,255,1)",
      "height": "70px"
    },
    ".guideHelpQuestionMark": {
      "border-radius": "9px",
      "height": "16px",
      "width": "16px",
      "top": "0px",
      "right": "8px"
    },
    ".guidePanelDescription > .guideHelpQuestionMark": {
      "position": "relative",
      "top": "0px",
      "left": "5px"
    },
    ".guideToolbarNode": {
      "margin": "24px 0px",
      "padding": "2px 0px"
    },
    ".afToolbarButton .guidebutton button:not(.af-icon-button)": {
      "margin-right": "8px",
      "margin-left": "0px"
    },
    ".guideTextDraw.guideStaticText": {
      "padding": "0px 12px",
      "min-height": "22px"
    },
    ".guideImage img": {
      "margin": "15px 12px 0px 0px"
    },
    "hr": {
      "margin": "8px 12px",
      "border-style": "solid",
      "border-color": "rgba(0,0,0,0.15)"
    },
    ".afFooter": {
      "text-align": "center",
      "background-color": "rgba(230,230,230,1)",
      "margin-top": "64px",
      "padding-top": "8px",
      "padding-bottom": "8px"
    },
    ".guideTextDraw.footerText": {
      "color": "rgba(150,150,150,1)",
      "font-size": "12px"
    }
  }
}
<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:lastModified="{Date}2018-10-04T09:50:29.650+02:00"
            cq:lastModifiedBy="admin"
            cq:template="/conf/${appId}/settings/wcm/templates/spa-page-template"
            jcr:primaryType="cq:PageContent"
            jcr:title="${appTitle} Sub Page"
            sling:resourceType="${appId}/components/page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="${appId}/components/container"
                layout="responsiveGrid">
            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="${appId}/components/container">
                <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="${appId}/components/title"/>
            </container>
        </root>
    </jcr:content>
</jcr:root>

<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"  xmlns:fd="http://www.adobe.com/aemfd/fd/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        guideComponentType="fd/af/templates"
        sling:resourceType="fd/af/components/page2/aftemplatedpage"
        jcr:primaryType="cq:PageContent">
        <guideContainer
            jcr:primaryType="nt:unstructured"
            fd:version="1.1"
            sling:resourceType="fd/af/components/guideContainer"
            guideCss="guideContainer"
            guideNodeClass="guideContainerNode"
            actionType="fd/af/components/guidesubmittype/restendpoint"
            themeRef="/content/dam/formsanddocuments-themes/${appId}/canvas-3-0"
            name="guide1">
            <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/layouts/defaultGuideLayout"
                toolbarPosition="Bottom"/>
            <rootPanel
                jcr:primaryType="nt:unstructured"
                sling:resourceType="fd/af/components/rootPanel"
                guideNodeClass="rootPanelNode"
                jcr:title="Root Panel"
                name="guideRootPanel"
                panelSetType="Navigable"
                dorExcludeTitle="true"
                dorExcludeDescription="true">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="fd/af/layouts/gridFluidLayout2"
                    nonNavigable="{Boolean}true"
                    toolbarPosition="Bottom"/>
                <items jcr:primaryType="nt:unstructured"/>
            </rootPanel>
        </guideContainer>
    </jcr:content>
</jcr:root>